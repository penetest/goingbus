page {
	height: 100%;
}
/* .container {
  height: 100%;
} */

.header {
	position: fixed;
	/* display: -webkit-flex; */
	left: 0;
	top: 0;
	width: 100%;
	padding: 0 20rpx 10rpx 20rpx;
	box-sizing: border-box;
	z-index: 1;
	/* -webkit-transition: background 0.5s ease-in-out; */
}
.header.scrolled {
	background: #feb70f;
}
.header .icon {
	width: 40rpx;
	height: 40rpx;
	vertical-align: top;
}
.header .location {
	color: #fff;
	font-size: 30rpx;
	/* line-height: 40rpx; */
	min-width: 100rpx;
	/* max-width: 340rpx; */
	padding: 0rpx 0rpx 10rpx 0rpx;
	border-radius: 30rpx;
	/* background: rgba(0,0,0,0.3); */
	margin-right: 20rpx;
	vertical-align: middle;
}
.header .search {
	-webkit-flex: 1;
	color: #888;
	font-size: 30rpx;
	line-height: 40rpx;
	padding: 10rpx;
	border-radius: 30rpx;
	background: #fff;
	/*border-bottom: 1rpx solid #aaa;*/
}

/* 最近记录 */
.recently-container{
  position: relative;
	top: 118rpx;
  padding-bottom: 90px;
  /* padding: 40px 0px 90px 0px; */
  width: 100%;
}
.recently {
  background-color: #fff;
  width: 100%;
}

.item {
  position: relative;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 0 10px;
  height: 40px;
  border-bottom: 1px solid #eaeaea;
  overflow: hidden;
}

.content {
  flex-grow: 1;
  display: flex;
  align-items: center;
}

.content-icon {
  margin-right: 8px;
  font-size: 24px;
  color: #0082dc;
}

.content-name {
  font-size: 14px;
  color: #333;
}

.delete {
  width: 26px;
  height: 26px;
  text-align: center;
  line-height: 26px; 
}

.delete-icon {
  font-size: 14px;
  color: #666;
}

.clear {
  height: 40px;
  text-align: center;
  line-height: 40px;
  font-size: 12px;
  color: #666;
  background-color: #fff;
}

.no-recently {
  margin-top: 100px;
  text-align: center;
  font-size: 16px;
  color: #aaa;
}


